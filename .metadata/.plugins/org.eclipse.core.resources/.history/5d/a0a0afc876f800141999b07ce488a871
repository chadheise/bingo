package bingo.checkers;

import bingo.Letter;
import bingo.interfaces.Card;
import bingo.interfaces.CardChecker;

public class VerticalChecker implements CardChecker {

	@Override
	public boolean hasWon(Card card) {
		for (Letter letter : Letter.values()) {
			for (int row=0; row < card.numRows(); row++) {
				card.getSpace(letter, row).isCovered();

			}
		}
		return false;
	}

	
	
}
