package bingo;

import java.util.ArrayList;
import java.util.List;

import bingo.interfaces.Column;
import bingo.interfaces.ColumnFactory;
import bingo.interfaces.Space;

public class ColumnFactoryImpl implements ColumnFactory {

	@Override
	public List<Space> createColumn(Letter letter) {
		List<Space> spaces = new ArrayList<Space>();
		for (int i=0; i < 5; i++) {
			Space = new SpaceImpl();
		}
		
		
	}

	private class ColumnImpl implements Column {

		private List<Space> spaces = new ArrayList<Space>(5);

		@Override
		public boolean isCoveredAtIndex(int i) {
			return spaces.get(i).isCovered();
		}
		
		private setSpace(int index, int value) {
			spaces.a
		}
		
	}
	
}
