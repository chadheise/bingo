package bingo;

import bingo.interfaces.CardFactory;
import bingo.utils.CardPrinter;

public class Main {

	private static final CardPrinter PRINTER = new CardPrinter();
	private static final boolean WITH_COVERS = false;
	
	public static void main(String[] args) {
		CardFactory cardFactory = new RandomCardFactory();
		PRINTER.print(cardFactory.createCard(), WITH_COVERS);
	}
	
}
