package bingo;

import bingo.interfaces.Card;

public class CardPrinter {

	public void print(Card card, boolean withCovers) {
		System.out.println("B  I  N  G  O  ");
		
	}
	
	private String getRow(boolean withCovers) {
		StringBuilder builder = new StringBuilder();
		for (int i=0; i < 5; i++) { // TODO: Don't hardcode 5, get it from the card
			
		}
		builder.append(card.getValue(Letter.B, 1)

	}
	
}
