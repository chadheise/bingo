package bingo;

public class Square {

	private int value;
	private boolean isCovered = false;
	
	public Square(int value) {
		this.value = value;
	}
	
	public int getValue() {
		return value;
	}
	
	public boolean isCovered() {
		return isCovered;
	}
	
	public void cover() {
		isCovered = true;
	}
	
}
