package bingo;

import java.util.ArrayList;
import java.util.List;

import bingo.interfaces.Column;
import bingo.interfaces.ColumnFactory;
import bingo.interfaces.Square;

public class ColumnFactoryImpl implements ColumnFactory {

	@Override
	public List<Square> createColumn(boolean hasFreeSpace) {
		// TODO Auto-generated method stub
		return null;
	}

	private class ColumnImpl implements Column {

		private List<Square> spaces = new ArrayList<Square>(5);

		@Override
		public boolean isCoveredAtIndex(int i) {
			return spaces.get(i).isCovered();
		}
		
	}
	
}
